FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            nutkWallFunction;
        value           nonuniform 0();
    }
    outlet
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    non_interface_walls
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    interface
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    symm_parts3
    {
        type            symmetryPlane;
    }
    symm_parts4
    {
        type            nutkWallFunction;
        value           nonuniform 0();
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0;
    }
}


